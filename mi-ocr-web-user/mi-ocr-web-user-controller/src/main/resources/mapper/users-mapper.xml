<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.millions.idea.ocr.web.user.repository.mapper.IUserMapperRepository">
    <sql id="fields">
       userName,password,issueId,issueResult,qq
    </sql>

    <select id="queryList" parameterType="java.util.List" resultType="org.millions.idea.ocr.web.user.entity.db.Users">
        SELECT * FROM users
    </select>

    <select id="query" parameterType="java.lang.Integer" resultType="org.millions.idea.ocr.web.user.entity.db.Users">
        SELECT * FROM users WHERE uid = #{uid}
    </select>
</mapper>